						"$ref": "#/definitions/HealthInsuranceInfo"
					}
				}
			}
		},
		"HealthInsuranceInfo": {
			"type": "object",
			"properties": {
                "code": {
                    "description": "Código do Plano de Saúde",
					"required": true,
					"type": "string",
					"example": "1$_$2$_$01",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZPLANOSCONVENIO.CODCOLIGADA|SZPLANOSCONVENIO.CODCOMPRADOR|SZPLANOSCONVENIO.CODPLANO",
							"required": true,
							"type": "string",
							"available": true,
							"canUpdate": false
						}
					]
                },
                "insuranceCompanyCode": {
                    "description": "Código da Empresa",
					"required": true,
					"type": "string",
					"example": "1$_$2",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZPLANOSCONVENIO.CODCOLIGADA|SZPLANOSCONVENIO.CODCOMPRADOR",
							"required": true,
							"type": "string",
							"available": true,
							"canUpdate": false
						}
					]
                },
                "planId": {
                    "description": "Id do Plano",
					"required": true,
                    "type": "string",
                    "maxLength": 3,
					"example": "01",
					"x-totvs": [
						{
							"product": "RM",
							"field": "SZPLANOSCONVENIO.CODPLANO",
							"required": true,
                            "type": "string",
                            "maxLength": 3,
							"available": true,
							"canUpdate": false
						}
					]
                },
                "companyId": {
                    "description": "Id da Coligada",
                    "required": true,
                    "type": "integer",
                    "example": 10,
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "SZPLANOSCONVENIO.CODCOLIGADA",
                            "required": true,
                            "type": "integer",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "insuranceCompanyId": {
                    "description": "Id da Empresa",
                    "required": true,
                    "type": "integer",
                    "example": 10,
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "SZPLANOSCONVENIO.CODCOMPRADOR",
                            "required": true,
                            "type": "integer",
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "description": {
                    "description": "Descrição do Plano",
                    "required": true,
                    "type": "string",
                    "maxLength": 255,
                    "example": "APARTAMENTO",
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "SZPLANOSCONVENIO.DESCPLANO",
                            "required": true,
                            "type": "string",
                            "maxLength": 255,
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                },
                "insuranceCompanyName": {
                    "description": "Nome da Empresa do Plano",
                    "required": true,
                    "type": "string",
                    "maxLength": 40,
                    "example": "TESTE Saúde",
                    "x-totvs": [
                        {
                            "product": "RM",
                            "field": "SZCADGERAL.SIGLA",
                            "required": true,
                            "type": "string",
                            "maxLength": 40,
                            "available": true,
                            "canUpdate": false
                        }
                    ]
                }
			}
		}
