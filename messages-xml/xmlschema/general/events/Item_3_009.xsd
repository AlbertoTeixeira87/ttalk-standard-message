						<FieldDocumentation product="PROTHEUS">
							<Field>SB1.B1_DESC</Field>
							<Required>sim</Required>
							<Type>char</Type>
							<Length>30</Length>
							<Description/>
						</FieldDocumentation>
						<FieldDocumentation product="RM">
							<Field>TPRODUTO.DESCRICAO</Field>
							<Required>não</Required>
							<Type>varchar</Type>
							<Length>240</Length>
							<Description>Descrição</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
