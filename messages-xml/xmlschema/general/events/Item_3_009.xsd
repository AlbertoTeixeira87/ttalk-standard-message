							<Type>varchar</Type>
							<Length>10</Length>
							<Description>Código Tabela de Classificação2 Produto</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
