					<xs:appinfo>
						<FieldDocumentation product="LOGIX">
						</FieldDocumentation>
						<FieldDocumentation product="DATASUL">
							<Field>item.cod-estabel</Field>
							<Required>sim</Required>
							<Type>char</Type>
							<Length>5</Length>
							<Description>Código do estabelecimento. Tabela do De-para padrão do EAI: estabelec</Description>
						</FieldDocumentation>
						<FieldDocumentation inputdocument="PROTHEUS">
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Code" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Código Item</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SB1.B1_COD</Field>
							<Required>sim</Required>
							<Type>char</Type>
							<Length>15</Length>
							<Description>campo expansivel ate 30</Description>
						</FieldDocumentation>
						<FieldDocumentation product="RM">
							<Field>TPRODUTO.CODIGOPRD</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>30</Length>
							<Description>Código do produto</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="InternalId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>InternalId de Integração</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SB1.B1_FILIAL+SB1.B1_COD</Field>
							<Required>sim</Required>
							<Type>char</Type>
						</FieldDocumentation>
						<FieldDocumentation product="RM">
							<Field>.</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>6</Length>
