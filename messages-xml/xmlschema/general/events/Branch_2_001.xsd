							<Field>SM0.M0_BAIRCOB</Field>
							<Required>Não</Required>
							<Type>Char</Type>
							<Length>35</Length>
							<Description>Campo obrigatório para o processo fiscal/TAF. </Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="BillingCity" type="xs:string">
