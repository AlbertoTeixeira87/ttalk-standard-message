					<xs:documentation>Código da Empresa</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>XX8.XX8_CODIGO</Field>
							<Required>Não</Required>
							<Type>Char</Type>
							<Length>12</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="UnitOfBusiness" type="xs:string">
