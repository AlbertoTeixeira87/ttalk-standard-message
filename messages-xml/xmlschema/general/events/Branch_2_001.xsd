		<xs:sequence>
			<xs:element name="Code" type="xs:string">
				<xs:annotation>
					<xs:documentation>Filial/Codigo Unidade</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ParentCode" type="xs:string">
				<xs:annotation>
					<xs:documentation>Coligada/Codigo Unidade Pai</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="xs:string">
				<xs:annotation>
					<xs:documentation>Descrição</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Title" type="xs:string">
