						<FieldDocumentation product="PROTHEUS">
							<Field>SM0.M0_CODMUN</Field>
							<Required>Não</Required>
							<Type>Char</Type>
							<Length>7</Length>
							<Description>Campo obrigatório para o processo fiscal/TAF. </Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CNAECode" type="xs:string">
