						<FieldDocumentation product="PROTHEUS">
							<Field>SM0.M0_CIDCOB</Field>
							<Required>Não</Required>
							<Type>Char</Type>
							<Length>60</Length>
							<Description>Campo obrigatório para o processo fiscal/TAF. </Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
