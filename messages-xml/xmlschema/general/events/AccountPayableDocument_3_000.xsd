<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
			xsi:noNamespaceSchemaLocation="../totvsmsgdoc.xsd" 
			elementFormDefault="qualified">

	<!-- ===================================== -->
	<!--  INCLUDES NECESSARIOS PARA A MENSAGEM -->
	<!-- ===================================== -->
	<xs:include schemaLocation="../totvsmsg.xsd"/>
	<xs:include schemaLocation="../types/ApportionType_2_004.xsd"/>
	<xs:include schemaLocation="../types/ListOfInternalId_1_000.xsd"/>

	<!-- ==================================== -->
	<!-- DOCUMENTAÇÃO DA MENSAGEM             -->
	<!-- ==================================== -->
	<xs:element name="BusinessContent" type="BusinessContentType" substitutionGroup="AbstractBusinessContent"/>
	<xs:annotation>
		<xs:appinfo>
			<MessageDocumentation>
				<Name>AccountPayableDocument</Name>
				<Description>Título a pagar</Description>
				<Segment>Construção e Projetos</Segment>
				<ProductInformation product="RM">
					<Contact>Alaim Porto Alvarenga</Contact>
					<Description>GDP Inovação Const. e Proj.</Description>
					<Adapter>MovMovimentoData</Adapter>
					<Send>
						<Insert>sim</Insert>
						<Update>não</Update>
						<Delete>sim</Delete>
					</Send>
					<Receive>
						<Insert>sim</Insert>
						<Update>sim</Update>
						<Delete>sim</Delete>
					</Receive>
				</ProductInformation>
				<ProductInformation product="PROTHEUS">
					<Contact>Rogerio Nagy</Contact>
					<Description>GDP de Inovação Controladoria</Description>
					<Adapter>FINI050</Adapter>
					<Send>
						<Insert>sim</Insert>
						<Update>sim</Update>
						<Delete>sim</Delete>
					</Send>
					<Receive>
						<Insert>sim</Insert>
						<Update>não</Update>
						<Delete>sim</Delete>
					</Receive>
				</ProductInformation>
			</MessageDocumentation>
		</xs:appinfo>
	</xs:annotation>

	<!-- ==================================== -->
	<!-- CONTEÚDO DE NEGÓCIO DA MENSAGEM      -->
	<!-- ==================================== -->
	<xs:complexType name="BusinessContentType">
		<xs:sequence>
			<xs:element name="CompanyId" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="BranchId" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="CompanyInternalId" type="xs:string" minOccurs="1" maxOccurs="1"/>
			<xs:element name="InternalId" type="xs:string" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>InternalId do Título</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODCOLIGADA|TMOV.IDMOV</Field>
							<Required>sim</Required>
							<Type>integer|integer</Type>
						</FieldDocumentation>
						<FieldDocumentation product="PROTHEUS">
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>50</Length>
							<Description>O InternalId do título é formado por E2_FILIAL|E2_PREFIXO|E2_NUM|E2_PARCELA|E2_TIPO|E2_FORNECE|E2_LOJA</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentPrefix" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Prefixo do título</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_PREFIXO</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>3</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentNumber" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Número do título</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_NUM</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>9</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentParcel" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Parcela do título</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_PARCELA</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>1</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="DocumentTypeCode" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Tipo do título</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_TIPO</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>1</Length>
						</FieldDocumentation>
					</xs:appinfo>        
				</xs:annotation>
			</xs:element>
			<xs:element name="IssueDate" type="xs:date" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Data de emissão</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.DATAEMISSAO</Field>
							<Required>sim</Required>
							<Type>datetime</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_EMISSAO</Field>
							<Required>sim</Required>
							<Type>date</Type>
							<Length>8</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="DueDate" type="xs:date" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Data de vencimento original</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.DATAEXTRA1</Field>
							<Required>sim</Required>
							<Type>datetime</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_VENCTO</Field>
							<Required>sim</Required>
							<Type>date</Type>
							<Length>8</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="RealDueDate" type="xs:date" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Data de vencimento real</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_VENCREA</Field>
							<Required>não</Required>
							<Type>date</Type>
							<Length>8</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorCode" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Código do fornecedor/cliente</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODCFO</Field>
							<Required>sim</Required>
							<Type>string</Type>
							<Lenght>25</Lenght>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_FORNECE</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>6</Length>
							<Description/>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="StoreId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Loja do fornecedor/cliente</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_LOJA</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>6</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorInternalId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>InternalId do fornecedor/cliente</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODCOLCFO|TMOV.CODCFO</Field>
							<Required>sim</Required>
							<Type>integer|string</Type>
							<Lenght>|25</Lenght>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>50</Length>
							<Description>O InternalId do fornecedor/cliente é formado por EMPRESA|A2_FILIAL|A2_COD|A2_LOJA|Entidade</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="NetValue" type="xs:double" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Valor líquido.</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.VALORBRUTO</Field>
							<Required>sim</Required>
							<Type>decimal</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_VALOR</Field>
							<Required>sim</Required>
							<Type>decimal</Type>
							<Length>16,2</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="GrossValue" type="xs:double" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Valor bruto</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.VALORBRUTO</Field>
							<Required>sim</Required>
							<Type>decimal</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_VALOR</Field>
							<Required>sim</Required>
							<Type>decimal</Type>
							<Length>16,2</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencyCode" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Código da moeda</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_MOEDA</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>2</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencyInternalId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>InternalId da moeda</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>50</Length>
							<Description>O InternalId da moeda é formado por EMPRESA|CTO_FILIAL|CTO_MOEDA</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CurrencyRate" type="xs:double" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Taxa da moeda utilizada na geração do título</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_TXMOEDA</Field>
							<Required>sim</Required>
							<Type>decimal</Type>
							<Length>16,2</Length>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Taxes" type="TaxesType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Impostos</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ApportionmentDistribution" minOccurs="0" maxOccurs="1">
				<xs:complexType>
					<xs:annotation>
						<xs:documentation> Distribuição de rateio.</xs:documentation>
					</xs:annotation>
					<xs:sequence>
						<xs:element name="Apportionment" type="ApportionType" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ListOfSourceDocument" type="ListOfSourceDocumentType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Notas fiscais do documento</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Observation" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Observação</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Origin" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Origem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FinancialNatureInternalId" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>InternalId da natureza</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>50</Length>
							<Description>O InternalId da natureza é formado por EMPRESA|ED_FILIAL|ED_CODIGO</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorBankCode" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Código do banco do fornecedor</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_FORBCO</Field>
							<Required>Nao</Required>
							<Type>String</Type>
							<Length>3</Length>
						</FieldDocumentation>
						<FieldDocumentation product="TFS">
							<Field>cd_cbr_lib|cd_bco|cd_bc</Field>
							<Required>Nao</Required>
							<Type>Integer</Type>
							<Length>10</Length>
							<Description>
							Código do Banco do Cliente:
							Campo cd_cbr_lib, se for a rotina "Titulos Pagar";
							Campo cd_bco, se for a rotina "Titulos Pagar Desconto";
							Campo cd_bco, se for a rotina "De Prazo Títulos Pagar" ou "De Prazo Títulos Pagar Exclusão";
							Campo cd_bc, se não houver dados bancários no título.
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorBankBranch" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Código da agência bancária do fornecedor</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_FORAGE</Field>
							<Required>Nao</Required>
							<Type>String</Type>
							<Length>5</Length>
						</FieldDocumentation>
						<FieldDocumentation product="TFS">
							<Field>cd_und_lib|cd_age|cd_ag|nr_ag</Field>
							<Required>Nao</Required>
							<Type>Integer</Type>
							<Length>10</Length>
							<Description>
							Código da Agência Bancária do cliente (no mesmo campo é salvo agência e DV):
							Campo cd_und_lib, se for a rotina "Titulos Pagar";
							Campo cd_age, se for a rotina "Titulos Pagar Desconto";
							Campo cd_ag, se for a rotina "De Prazo Títulos Pagar" ou "De Prazo Títulos Pagar Exclusão";
							Campo nr_ag, se não houver dados bancários no título.
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorBankBranchKey" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Dígito verificador da agência bancária do fornecedor</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_FAGEDV</Field>
							<Required>Nao</Required>
							<Type>String</Type>
							<Length>1</Length>
						</FieldDocumentation>
						<FieldDocumentation product="TFS">
							<Field>cd_und_lib|cd_age|cd_ag|nr_ag</Field>
							<Required>Nao</Required>
							<Type>Integer</Type>
							<Length>10</Length>
							<Description>
							Digito Verificador da Agência Bancária do cliente (no mesmo campo é salvo agência e DV):
							Campo cd_und_lib, se for a rotina "Titulos Pagar";
							Campo cd_age, se for a rotina "Titulos Pagar Desconto";
							Campo cd_ag, se for a rotina "De Prazo Títulos Pagar" ou "De Prazo Títulos Pagar Exclusão";
							Campo nr_ag, se não houver dados bancários no título.
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorCheckingAccountNumber" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Número da conta bancária do fornecedor</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_FORCTA</Field>
							<Required>Nao</Required>
							<Type>String</Type>
							<Length>10</Length>
							<Description>Conta Bancária do Fornecedor.</Description>
						</FieldDocumentation>
						<FieldDocumentation product="TFS">
							<Field>cd_cc_lib|cd_cco|cd_cta|nr_cc</Field>
							<Required>Nao</Required>
							<Type>string</Type>
							<Length>12</Length>
							<Description>
							Código da Conta Bancária do cliente (no mesmo campo é salvo conta e DV):
							Campo cd_cc_lib, se for a rotina "Titulos Pagar";
							Campo cd_cco, se for a rotina "Titulos Pagar Desconto";
							Campo cd_cta, se for a rotina "De Prazo Títulos Pagar" ou "De Prazo Títulos Pagar Exclusão";
							Campo nr_cc, se não houver dados bancários no título.
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="VendorCheckingAccountNumberKey" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Dígito Verificador da Conta Bancária do Fornecedor</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>E2_FCTADV</Field>
							<Required>Nao</Required>
							<Type>String</Type>
							<Length>1</Length>
							<Description>Digito Verificador da Conta Bancária do Fornecedor.</Description>
						</FieldDocumentation>
						<FieldDocumentation product="TFS">
							<Field>cd_cc_lib|cd_cco|cd_cta|nr_cc</Field>
							<Required>Nao</Required>
							<Type>string</Type>
							<Length>12</Length>
							<Description>
							Digito Verificador da Conta Bancária do cliente (no mesmo campo é salvo conta e DV):
							Campo cd_cc_lib, se for a rotina "Titulos Pagar";
							Campo cd_cco, se for a rotina "Titulos Pagar Desconto";
							Campo cd_cta, se for a rotina "De Prazo Títulos Pagar" ou "De Prazo Títulos Pagar Exclusão";
							Campo nr_cc, se não houver dados bancários no título.
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="Accounting" type="AccountingType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Dados da contabilização</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="ListOfComplementaryValues" type="ListOfComplementaryValueType" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Lista de valores complementares</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ListOfSourceDocumentType">
		<xs:sequence>
			<xs:element name="SourceDocument" type="SourceDocumentType" maxOccurs="unbounded" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="SourceDocumentType">
		<xs:sequence>
			<xs:element name="SourceDocument" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Documento de Origem. Número do Documento Relacionado ao Título</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="10"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SourceDocumentSerie" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Serie</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SourceDocumentSubSerie" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>SubSerie</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="SourceDocumentTypeCode" type="xs:string" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Tipo de Documento de Origem. Tipo do Documento Relacionado ao Título</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceDocumentValue" type="xs:double" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Valor do Documento de Origem. Valor do Documento Relacionado ao Título</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaxesType">
		<xs:sequence>
			<xs:element name="Tax" type="TaxType" maxOccurs="unbounded" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TaxType">
