<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	elementFormDefault="qualified"
	xsi:noNamespaceSchemaLocation="../totvsmsgdoc.xsd">

	<!-- ===================================== -->
	<!--  INCLUDES NECESSARIOS PARA A MENSAGEM -->
	<!-- ===================================== -->
	<xs:include schemaLocation="../totvsmsg.xsd" />
	<xs:include schemaLocation="../types/Address_1_000.xsd" />
	<xs:include	schemaLocation="../types/GovernmentalInformation_1_000.xsd" />
	<xs:include schemaLocation="../types/ApportionType_2_001.xsd" />
	<xs:include schemaLocation="../types/ListOfInternalId_1_000.xsd" />
	<xs:include schemaLocation="../types/AssignmentTypeEnum_1_000.xsd" />
	<xs:include schemaLocation="../totvsmsgdoc.xsd" />
	
	<!-- ==================================== -->
	<!-- DOCUMENTAÇÃO DA MENSAGEM             -->
	<!-- ==================================== -->
	
	<xs:annotation>
		<xs:appinfo>
			<MessageDocumentation>
				<Name>Order</Name>
				<Description>Pedido de compra</Description>
				<Segment>Construção e Projetos</Segment>
				<ProductInformation product="RM">
					<Contact>Alaim Porto Alvarenga</Contact>
					<Description>
						GDP Inovação Const. e Proj.
					</Description>
					<Adapter>MovMovimentoData</Adapter>
					<Send>
						<Insert>sim</Insert>
						<Update>não</Update>
						<Delete>sim</Delete>
					</Send>
					<Receive>
						<Insert>sim</Insert>
						<Update>sim</Update>
						<Delete>sim</Delete>
					</Receive>
				</ProductInformation>
				<ProductInformation product="PROTHEUS">
					<Contact>Eduardo de Souza</Contact>
					<Description>GDP de Materiais</Description>
					<Adapter>MATA120</Adapter>
					<Send>
						<Insert>sim</Insert>
						<Update>sim</Update>
						<Delete>sim</Delete>
					</Send>
					<Receive>
						<Insert>sim</Insert>
						<Update>não</Update>
						<Delete>sim</Delete>
					</Receive>
				</ProductInformation>
			</MessageDocumentation>
			<MessageDocumentation>
				<Name>Order</Name>
				<Description>Pedido de venda</Description>
				<Segment>Construção e Projetos</Segment>
				<ProductInformation product="RM">
					<Contact>Alaim Porto Alvarenga</Contact>
					<Description>
						GDP Inovação Const. e Proj.
					</Description>
					<Adapter>MovMovimentoData</Adapter>
					<Send>
						<Insert>sim</Insert>
						<Update>não</Update>
						<Delete>sim</Delete>
					</Send>
					<Receive>
						<Insert>sim</Insert>
						<Update>sim</Update>
						<Delete>sim</Delete>
					</Receive>
				</ProductInformation>
				<ProductInformation product="PROTHEUS">
					<Contact>Hanna Caroline Inglez</Contact>
					<Description>GDP de Serviços</Description>
					<Adapter>MATA410</Adapter>
					<Send>
						<Insert>sim</Insert>
						<Update>sim</Update>
						<Delete>sim</Delete>
					</Send>
					<Receive>
						<Insert>sim</Insert>
						<Update>não</Update>
						<Delete>sim</Delete>
					</Receive>
				</ProductInformation>
			</MessageDocumentation>
		</xs:appinfo>
	</xs:annotation>

	<!-- ==================================== -->
	<!-- CONTEÚDO DE NEGÓCIO DA MENSAGEM      -->
	<!-- ==================================== -->
	<xs:complexType name="TcHeader">
		<xs:sequence>
			<xs:element name="CompanyInternalId" type="xs:string">
				<xs:annotation>
					<xs:documentation>
						InternalId da Empresa
					</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>
								TMOV.CODCOLIGADA|TMOV.CODFILIAL
							</Field>
							<Required>sim</Required>
							<Type>integer|integer</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field />
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>50</Length>
							<Description>
								o InternalID da empresa é formado por
								EMPRESA|F1_FILIAL. Tamanho entre 05 e 15
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="CompanyId" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Empresa</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODCOLIGADA</Field>
							<Required>sim</Required>
							<Type>integer</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>
								Não possui campo físico na tabela
							</Field>
							<Required>Sim</Required>
							<Type>varchar</Type>
							<Length>2</Length>
							<Description />
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="BranchId" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Filial</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODFILIAL</Field>
							<Required>sim</Required>
							<Type>integer</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SC7XX0.C7_FILIAL</Field>
							<Required>Sim</Required>
							<Type>varchar</Type>
							<Length>2</Length>
							<Description>
								Pode ter tamanho entre 02 e 12
								dependendo da configuração do Protheus
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="funcmsgorder" type="tsfuncmsgorder"
				minOccurs="0" />

			<xs:element name="OrderPurpose" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Objetivo do pedido
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation>
									Compra
								</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation>
									Venda
								</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="ordertypecode" type="tsordertypecode"
				minOccurs="0" />

			<xs:element name="CustomerOrderId" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Número do Pedido do Cliente (no LOGIX esta
						informação fica na capa do pedido)
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="InternalId" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						InternalId do Pedido
					</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODCOLIGADA|TMOV.IDMOV</Field>
							<Required>sim</Required>
							<Type>integer|integer</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field />
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>50</Length>
							<Description>
								O InternalId é formado por FILIAL|C7_NUM
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="RegisterDate" type="xs:date"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Data de Emissao</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.DATAEMISSAO</Field>
							<Required>sim</Required>
							<Type>DateTime</Type>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SC7XX0.C7_EMISSAO</Field>
							<Required>não</Required>
							<Type>date</Type>
							<Length>8</Length>
							<Description />
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="OrderId" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						No.do Pedido (Deve ser enviado somente para
						alteracao e exclusao)
					</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SC7XX0.C7_NUM</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>6</Length>
							<Description />
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="Version" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Versão do Pedido de Compra
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="RegisterHour" type="xs:time"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Hora de Emissao</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="dhinidelivery" type="tsDataTempo"
				minOccurs="0" />

			<xs:element name="dhfindelivery" type="tsDataTempo"
				minOccurs="0" />

			<xs:element name="RequestDate" type="xs:date"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Data da solicitação do pedido
						Cliente/Representante
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="FreightType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Tipo de Frete</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation>CIF</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation>FOB</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="3">
							<xs:annotation>
								<xs:documentation>SFT</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="FreightValue" type="xs:double"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Frete</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SC7XX0.C7_VALFRE</Field>
							<Required>não</Required>
							<Type>decimal</Type>
							<Length>14,2</Length>
							<Description>
								Contém a máscara numérica @E
								999,999,999.99
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="RedeliveryCarrierCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Redespacho</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="InvoiceMessages" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Mensagem da Nota Fiscal
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="NetWeight" type="xs:double"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Peso Líquido</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="GrossWeight" type="xs:double"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Peso Bruto</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SC7XX0.C7_PESO_B</Field>
							<Required>não</Required>
							<Type>decimal</Type>
							<Length>13,3</Length>
							<Description>
								Contém a máscara numérica @E
								999,999,999.999
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="TareWeight" type="xs:double"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Peso Tara</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="CubicVolume" type="xs:double"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>Volume Cúbico</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="NumberOfVolumes" type="xs:double"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Número de volumes
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="Finality" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Finalidade</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="1">
							<xs:annotation>
								<xs:documentation>
									Contribuinte ICMS
									(Indústria/Comércio)
								</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="2">
							<xs:annotation>
								<xs:documentation>
									Não-contribuinte ICMS
								</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="InsuranceValue" type="xs:double"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Valor do Seguro (Conta e Ordem)
					</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SC7XX0.C7_SEGURO</Field>
							<Required>não</Required>
							<Type>decimal</Type>
							<Length>14,2</Length>
							<Description>
								Contém a máscara numérica @E
								999,999,999.99
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>

			<xs:element name="CurrencyCode" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						LOGIX: Moeda do Pedido
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="CurrencyId" type="tscurrency"
				minOccurs="0" />

			<xs:element name="currencydesc" type="tscurrencydesc"
				minOccurs="0" />

			<xs:element name="Status" type="tsstatustype" minOccurs="0" />

			<xs:element name="contractnumber" type="tscontractnumber"
				minOccurs="0" />

			<xs:element name="pricetablenumber" type="tspricetablenumber"
				minOccurs="0" />

			<xs:element name="VendorGovInfo" type="GovernmentalInformationType"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Inscrição Estadual do fornecedor que será
						utilizada para gerar o pedido
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element name="CustomerCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Código do cliente/fornecedor
					</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODCFO</Field>
							<Required>sim</Required>
							<Type>string</Type>
							<Length>25</Length>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SC7XX0.C7_FORNECE</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>6</Length>
							<Description />
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>

			<xs:element name="CustomerInternalId" type="xs:string"
				minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						InternalId do cliente/fornecedor
					</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODCOLCFO|TMOV.CODCFO</Field>
							<Required>sim</Required>
							<Type>integer|string</Type>
							<Length>|25</Length>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field />
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>50</Length>
							<Description>
								O InternalId do fornecedor é formado por
								EMPRESA|A2_FILIAL|A2_COD|A2_LOJA
							</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="CustomerGovInfo" type="GovernmentalInformationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Documentos do cliente/comprador </xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="DeliveryCustomerCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Código do Cliente de Entrega</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
			<xs:element name="DeliveryCustomerGovernmentalInformation" type="GovernmentalInformationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Documentos do cliente/comprador para o qual será feita a entrega</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="DeliveryAddress" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Endereço de Entrega</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="BillingLocationGovInfo" type="GovernmentalInformationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Documentos do cliente/comprador para o qual será feita a entrega</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="CarrierGovInfo" type="GovernmentalInformationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Documentos do transportador</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="CarrierCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Código da Transportadora</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="15"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
			<xs:element name="carriername" type="tscarriername" minOccurs="0"/>
			
			<xs:element name="comment" type="tscomment" minOccurs="0"/>
			
			<xs:element name="natureoperation" type="tsnatureoperation" minOccurs="0"/>
			
			<xs:element name="UserInternalId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>InternalId do Usuario</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="LOGIX">
							<Field>Comprador.Login</Field>
							<Required>sim</Required>
							<Type>string</Type>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="conditionpay" type="tsconditionpay" minOccurs="0"/>
			
			<xs:element name="PaymentTermCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Condição de Pagamento</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>TMOV.CODCPG</Field>
							<Required>sim</Required>
							<Type>string</Type>
							<Length>5</Length>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field>SC7XX0.C7_COND</Field>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>3</Length>
							<Description/>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
			<xs:element name="PaymentConditionInternalId" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>InternalId da condição de pagamento</xs:documentation>
					<xs:appinfo>
						<FieldDocumentation product="RM">
							<Field>CODCOLIGADA|TMOV.CODCPG</Field>
							<Required>sim</Required>
							<Type>integer|string</Type>
							<Length>|5</Length>
						</FieldDocumentation>
					</xs:appinfo>
					<xs:appinfo>
						<FieldDocumentation product="PROTHEUS">
							<Field/>
							<Required>sim</Required>
							<Type>varchar</Type>
							<Length>50</Length>
							<Description>O InternalId da Condição de Pagamento é formado por EMPRESA|E4_FILIAL|E4_CODIGO</Description>
						</FieldDocumentation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="descconditionpay" type="tsdesccondition" minOccurs="0"/>
			
			<xs:element name="Discounts" type="DiscountType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Descontos</xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="FinancialDiscount" type="xs:double" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Desconto Financeiro</xs:documentation>
				</xs:annotation>
